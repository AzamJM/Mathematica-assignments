Cell[TextData[{
 "Write down the general solution to the quadratic equation (i.e. solve for \
x: a ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["x", "2"], TraditionalForm]],ExpressionUUID->
  "87061cf1-3f7c-4b7c-8998-1fe5b52c04b3"],
 " + b x + c = 0). Show using replacement rules how you can evaluate the \
solution for different \[OpenCurlyDoubleQuote]a,b,c\[CloseCurlyDoubleQuote] \
without re-entering the algebraic solution. In particular, show the result \
for the following three cases:"
}], "Item",
 CellChangeTimes->{{3.663963614564537*^9, 3.663963768392927*^9}, {
  3.6639638442555847`*^9, 3.663963845020865*^9}, {3.663963877662621*^9, 
  3.66396387854837*^9}, {3.6639639918520403`*^9, 3.663964009139032*^9}, {
  3.663966551310217*^9, 3.66396655366748*^9}, {3.666097875641899*^9, 
  3.6660979862496557`*^9}, {3.666156124760146*^9, 3.6661561321568403`*^9}, {
  3.7475922010538816`*^9, 
  3.747592207452044*^9}},ExpressionUUID->"a806dd74-3d37-49c9-803e-\
332d6eab90ad"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"equationq", " ", "=", " ", 
  RowBox[{"Roots", "[", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"a", " ", 
       RowBox[{"x", "^", "2"}]}], " ", "+", " ", 
      RowBox[{"b", " ", "x"}], "+", "c"}], " ", "\[Equal]", " ", "0"}], ",", 
    " ", "x"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.748847568772005*^9, 
  3.748847600599989*^9}},ExpressionUUID->"1ff050f8-a4f4-484e-9cd1-\
ab674d9f3756"],

Cell[BoxData[
 RowBox[{
  RowBox[{"x", "\[Equal]", 
   FractionBox[
    RowBox[{
     RowBox[{"-", "b"}], "-", 
     SqrtBox[
      RowBox[{
       SuperscriptBox["b", "2"], "-", 
       RowBox[{"4", " ", "a", " ", "c"}]}]]}], 
    RowBox[{"2", " ", "a"}]]}], "||", 
  RowBox[{"x", "\[Equal]", 
   FractionBox[
    RowBox[{
     RowBox[{"-", "b"}], "+", 
     SqrtBox[
      RowBox[{
       SuperscriptBox["b", "2"], "-", 
       RowBox[{"4", " ", "a", " ", "c"}]}]]}], 
    RowBox[{"2", " ", "a"}]]}]}]], "Output",
 CellChangeTimes->{3.7487955689037833`*^9, 3.7488475746252365`*^9},
 CellLabel->"Out[5]=",ExpressionUUID->"dea53607-2f6a-4bee-ae60-1a8ced4d9890"]
}, Open  ]],

